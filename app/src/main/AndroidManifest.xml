<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hcucare">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/hcucarelogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ambulancestatus"></activity>
        <activity android:name=".Ambulance" />
        <activity android:name=".Appointmentstatus" />
        <activity android:name=".AppointmentActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".MainDashboard" />
        <activity android:name=".SendPassword" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".DoctorsActivity" />
        <activity android:name=".Login" />
        <activity android:name=".NewsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>