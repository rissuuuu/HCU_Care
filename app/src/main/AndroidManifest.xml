<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hcucare">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.hcucare.NotificationActivity" />
        <activity android:name="com.example.hcucare.MainDashboard" />
        <activity android:name="com.example.hcucare.SendPassword" />
        <activity android:name="com.example.hcucare.ChangePassword" />
        <activity android:name="com.example.hcucare.Dashboard" />
        <activity android:name="com.example.hcucare.Login" />
        <activity android:name="com.example.hcucare.NewsActivity"/>
        <activity android:name="com.example.hcucare.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>